/dts-v1/;
#include "imx6q-phytec-pfla02.dtsi"

/ {
	model = "Phytec phyBOARD ALCOR";
	compatible = "phytec,imx6q-pbab02", "phytec,imx6q-pfla02", "fsl,imx6q";

	chosen {
		stdout-path = &uart4;
	};
};

&ethphy {
	max-speed = <100>;
};

&fec {
	status = "okay";
};

&uart4 {
	status = "okay";
};

&usdhc3 {
	status = "okay";
};
